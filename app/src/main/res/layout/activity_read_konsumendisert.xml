<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/coba1"
    android:layout_height="match_parent">



    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv_mainkonsumendisert"
        android:layout_width="match_parent"
        android:layout_height="508dp"
        android:scrollbars="vertical"
        tools:listitem="@layout/item_konsumen"/>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@+id/rv_mainkonsumendisert">

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/floatingmakanan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:layout_margin="@dimen/cardview_compat_inset_shadow"
            android:backgroundTint="@color/cardview_shadow_start_color"
            android:src="@drawable/iconmakanan" />
        <android.support.design.widget.FloatingActionButton
            android:id="@+id/floatinghome"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:layout_margin="@dimen/cardview_compat_inset_shadow"
            android:backgroundTint="@color/cardview_shadow_start_color"
            android:src="@drawable/ic_home_black_24dp" />
        <android.support.design.widget.FloatingActionButton
            android:id="@+id/floatingminuman"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:layout_margin="@dimen/cardview_compat_inset_shadow"
            android:backgroundTint="@color/cardview_shadow_start_color"
            android:src="@drawable/iconminuman" />


    </LinearLayout>



</RelativeLayout>